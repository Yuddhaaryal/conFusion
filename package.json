{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "clean": "rimraf dist",
    "imagemin": "imagemin img/* --out-dir='dist/img'",
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "usemin": "usemin index.html -d dist -o dist/index.html && usemin contactus.html -d dist -o dist/contactus.html",
    "build": "npm run clean && npm run imagemin && npm run copyfonts && npm run usemin"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cssmin": "^0.4.3",
    "grunt": "^1.0.2",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-sass": "^2.1.0",
    "htmlmin": "0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.3.0",
    "node-sass": "^4.14.1",
    "onchange": "^7.0.2",
    "parallelshell": "^3.0.1",
    "rimraf": "^3.0.2",
    "time-grunt": "^1.4.0",
    "uglify-js": "^2.8.29",
    "usemin-cli": "^0.5.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.5.0",
    "popper": "^1.0.1",
    "popper.js": "^1.14.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yuddhaaryal/conFusion.git"
  },
  "bugs": {
    "url": "https://github.com/Yuddhaaryal/conFusion/issues"
  },
  "homepage": "https://github.com/Yuddhaaryal/conFusion#readme"
}
